{
  "name": "@vip-gw2-guilds/gw2-palette-mapper",
  "version": "0.3.0",
  "description": "Palette mapping implementation for GW2 official build codes using the GW2 API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ViP-GW2-Guilds/gw2-palette-mapper.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src test",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "keywords": [
    "gw2",
    "guild-wars-2",
    "build-template",
    "palette-mapper",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.9.0",
  "peerDependencies": {
    "@vip-gw2-guilds/gw2-build-decoder": "^0.2.0 || ^0.3.0 || ^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vip-gw2-guilds/gw2-build-decoder": "^0.2.0",
    "eslint": "^9.39.2",
    "prettier": "^3.8.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
